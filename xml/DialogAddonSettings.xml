        <height>250</height>
        <itemgap>2</itemgap>
        <onleft>noop</onleft>
        <onright>2</onright>
        <onup>9</onup>
        <ondown>noop</ondown>
        <align>center</align>
        <aligny>top</aligny>
        <orientation>vertical</orientation>
        <control type="button" id="10" description="OK Button">
          <include>FocusButtonsAnimation</include>
          <width>360</width>
          <label>186</label>
        </control>
        <control type="button" id="12" description="Load Defaults">
          <include>FocusButtonsAnimation</include>
          <width>360</width>
          <label>409</label>
        </control>
      </control>
      <control type="group" description="Right Panel">
        <left>700</left>
        <top>120</top>
        <control type="grouplist" id="2" description="Right controls">
          <left>0</left>
          <top>0</top>
          <width>1125</width>
          <height>900</height>
          <itemgap>4</itemgap>
          <onup>2</onup>
          <ondown>2</ondown>
          <onleft>9</onleft>
          <onright>60</onright>
          <pagecontrol>60</pagecontrol>
        </control>
        <control type="button" id="3" description="Default Button">
          <include>FocusButtonsAnimation</include>
