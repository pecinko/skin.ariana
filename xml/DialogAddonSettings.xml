<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">9</defaultcontrol>
  <controls>
    <include>DialogFullBG</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="image" description="Sidebar">
        <left>-150</left>
        <top>0</top>
        <width>780</width>
        <height>100%</height>
        <aspectratio>stretch</aspectratio>
        <texture border="60">img/HomeMenuBarV.png</texture>
      </control>
      <control type="label" id="20" description="Heading">
        <include>CenterHeading</include>
      </control>
      <control type="grouplist" id="9" description="Left Configure Panel">
        <left>66</left>
        <top>120</top>
        <width>480</width>
        <height>585</height>
        <itemgap>4</itemgap>
        <onleft>30</onleft>
        <onright>2</onright>
        <onup>9</onup>
        <ondown>8999</ondown>
      </control>
      <control type="button" id="13" description="Default Category Button">
        <include>FocusButtonsAnimation</include>
        <width>480</width>
        <align>left</align>
      </control>
      <control type="grouplist" id="8999" description="Left OK/Cancel Panel">
